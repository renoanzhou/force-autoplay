{
  "name": "force-autoplay",
  "version": "1.2.1",
  "description": "基于浏览器自动播放的规则，实现媒体的自动播放, 并支持校验媒体元素是否允许自动播放，支持PC和移动端",
  "main": "dist/force-autoplay.cjs.js",
  "module": "dist/force-autoplay.es.js",
  "browser": "dist/force-autoplay.min.js",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c ./build/rollup.config.js",
    "dev": "rollup -c ./build/rollup.config.js -w -m",
    "doc": "typedoc ./src/index.ts",
    "npm-publish": "node npm-publish.js"
  },
  "keywords": [
    "autoplay",
    "video",
    "media",
    "自动播放"
  ],
  "typings": "dist/types/index.d.ts",
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "@rbnlffl/rollup-plugin-eslint": "^3.0.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "address": "^1.1.2",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "rollup": "^2.67.1",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  },
  "author": {
    "name": "zackXiong",
    "email": "zackisxiong@gmail.com"
  },
  "homepage": "https://github.com/renoanzhou/force-autoplay",
  "repository": {
    "type": "git",
    "url": "git@github.com:renoanzhou/force-autoplay.git"
  }
}
